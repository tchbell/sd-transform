{
  "semantic": {
    "fontSize": {
      "text": {
        "md": {
          "value": "{fontSize.f0}*{util.fontsizeAdjust.text}",
          "type": "fontSizes"
        },
        "sm": {
          "value": "{fontSize.f-1}*{util.fontsizeAdjust.text}",
          "type": "fontSizes"
        },
        "xs": {
          "value": "{fontSize.f-2}*{util.fontsizeAdjust.text}",
          "type": "fontSizes"
        }
      },
      "title": {
        "md": {
          "value": "{fontSize.f2}*{util.fontsizeAdjust.title}",
          "type": "fontSizes"
        },
        "lg": {
          "value": "{fontSize.f4}*{util.fontsizeAdjust.title}",
          "type": "fontSizes"
        },
        "xl": {
          "value": "{fontSize.f7} *{util.fontsizeAdjust.title}",
          "type": "fontSizes"
        },
        "xxl": {
          "value": "{fontSize.f9}*{util.fontsizeAdjust.title}",
          "type": "fontSizes"
        }
      }
    }
  }
}