{
  "pagination": {
    "text": {
      "typography": {
        "value": "{semantic.typography.pagination.text}",
        "type": "typography"
      },
      "gap": {
        "value": "{semantic.spacing.pagination.text.gap}",
        "type": "spacing"
      }
    },
    "default": {
      "text-color": {
        "value": "{semantic.color.pagination.default.text}",
        "type": "color"
      },
      "active": {
        "scrubber": {
          "line-color": {
            "value": "{semantic.color.pagination.default.active.scrubber.line}",
            "type": "color"
          }
        },
        "slider": {
          "line-color": {
            "value": "{semantic.color.pagination.default.active.slider.line}",
            "type": "color"
          },
          "bg-color": {
            "value": "{semantic.color.pagination.default.active.slider.bg}",
            "type": "color"
          },
          "icon-color": {
            "value": "{semantic.color.pagination.default.active.slider.icon}",
            "type": "color"
          }
        }
      },
      "rested": {
        "scrubber": {
          "line-color": {
            "value": "{semantic.color.pagination.default.rested.scrubber.line}",
            "type": "color"
          }
        },
        "slider": {
          "line-color": {
            "value": "{semantic.color.pagination.default.rested.slider.line}",
            "type": "color"
          },
          "icon-color": {
            "value": "{semantic.color.pagination.default.rested.slider.icon}",
            "type": "color"
          }
        }
      }
    },
    "rested": {
      "scrubber": {
        "40": {
          "value": "0.4",
          "type": "opacity"
        }
      }
    },
    "inverse": {
      "text-color": {
        "value": "{semantic.color.pagination.inverse.text}",
        "type": "color"
      },
      "active": {
        "scrubber": {
          "line-color": {
            "value": "{semantic.color.pagination.inverse.active.scrubber.line}",
            "type": "color"
          }
        },
        "slider": {
          "line-color": {
            "value": "{semantic.color.pagination.inverse.active.slider.line}",
            "type": "color"
          },
          "bg-color": {
            "value": "{semantic.color.pagination.inverse.active.slider.bg}",
            "type": "color"
          },
          "icon-color": {
            "value": "{semantic.color.pagination.inverse.active.slider.icon}",
            "type": "color"
          }
        }
      },
      "rested": {
        "scrubber": {
          "line-color": {
            "value": "{semantic.color.pagination.inverse.rested.scrubber.line}",
            "type": "color",
            "$extensions": {
              "studio.tokens": {
                "modify": {
                  "type": "alpha",
                  "value": "{pagination.rested.scrubber.40}",
                  "space": "lch"
                }
              }
            }
          }
        },
        "slider": {
          "line-color": {
            "value": "{semantic.color.pagination.inverse.rested.slider.line}",
            "type": "color"
          },
          "icon-color": {
            "value": "{semantic.color.pagination.inverse.rested.slider.icon}",
            "type": "color"
          }
        }
      }
    },
    "scrubber": {
      "gap": {
        "value": "{semantic.spacing.pagination.scrubber.gap}",
        "type": "spacing"
      }
    },
    "slider": {
      "gap": {
        "value": "{semantic.spacing.pagination.slider.gap}",
        "type": "spacing"
      },
      "bg-color": {
        "value": "#fff",
        "type": "color"
      }
    }
  },
  "pagintation": {
    "scrubber": {
      "height": {
        "value": "{semantic.sizing.scrubber.height}",
        "type": "sizing"
      },
      "lg": {
        "width": {
          "value": "{semantic.sizing.scrubber.fullWidth.width}",
          "type": "sizing"
        }
      },
      "sm": {
        "width": {
          "value": "{semantic.sizing.scrubber.sm.width}",
          "type": "sizing"
        }
      }
    }
  }
}